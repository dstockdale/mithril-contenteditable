{
  "name": "example",
  "private": true,
  "version": "0.0.1",
  "description": "Example project voor the mithril-contenteditable library.",
  "scripts": {
    "clean": "rimraf ./.cache",
    "start": "parcel index.html --out-dir ../../docs",
    "build:domain": "npm run clean && parcel build index.html --out-dir ../../docs --public-url https://erikvullings.github.io/mithril-contenteditable",
    "link": "npm link mithril-contenteditable",
    "build": "npm run clean && parcel build index.html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "example",
    "mithril",
    "typescript",
    "contenteditable"
  ],
  "author": "Erik Vullings <erik.vullings@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "mithril": "^2.0.4",
    "mithril-contenteditable": "^0.1.2",
    "material-icons": "^0.3.1",
    "materialize-css": "^1.0.0",
    "mithril-materialized": "^0.16.4"
  },
  "devDependencies": {
    "@types/materialize-css": "^1.0.8",
    "@types/mithril": "^2.0.4",
    "rimraf": "^3.0.2",
    "autoprefixer": "^10.0.0",
    "cssnano": "^4.1.10",
    "parcel-bundler": "^1.12.4",
    "typescript": "^4.0.3"
  }
}